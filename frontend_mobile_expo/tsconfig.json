{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./app/*", "./components/*"]
    },
    "esModuleInterop": true,
    "jsx": "react-native",
    "moduleResolution": "node",
    "target": "esnext",
    "lib": ["esnext"],
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["**/*.ts", "**/*.tsx", "./declarations.d.ts"]
}